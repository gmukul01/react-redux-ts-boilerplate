{
    "name": "react-redux-ts-boilerplate",
    "version": "1.0.0",
    "description": "React redux typescript boilerplate",
    "private": true,
    "sideEffects": false,
    "scripts": {
        "build": "webpack --progress --config config/webpack/webpack.prod.js",
        "build:analyze": "cross-env BUNDLE_ANALYZE=true npm run build",
        "lint": "run-p -c lint:**",
        "lint:css": "stylelint src/**/*.{ts,tsx}",
        "lint:ts": "tslint src/**/*.{ts,tsx} -p tsconfig.json",
        "test": "run-p type-check test:jest",
        "test:update": "run-p type-check \"test:jest -- -u\"",
        "test:watch": "run-p \"type-check -- -w\" \"test:jest -- --watch\"",
        "type-check": "tsc",
        "test:jest": "jest --config config/jest/jest.config.js",
        "watch": "webpack-dev-server --open --config config/webpack/webpack.dev.js"
    },
    "keywords": [],
    "author": "Mukul Bansal",
    "license": "ISC",
    "dependencies": {
        "@babel/polyfill": "^7.4.3",
        "axios": "^0.18.0",
        "react": "^16.8.4",
        "react-dom": "^16.8.4",
        "react-redux": "^6.0.1",
        "redux": "^4.0.1",
        "redux-saga": "^1.0.2",
        "styled-components": "^4.1.3"
    },
    "devDependencies": {
        "@babel/cli": "^7.2.3",
        "@babel/core": "^7.3.4",
        "@babel/plugin-proposal-class-properties": "^7.3.4",
        "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
        "@babel/preset-env": "^7.3.4",
        "@babel/preset-react": "^7.0.0",
        "@babel/preset-typescript": "^7.3.3",
        "@hot-loader/react-dom": "^16.8.4",
        "@types/enzyme": "^3.9.0",
        "@types/enzyme-adapter-react-16": "^1.0.5",
        "@types/enzyme-to-json": "^1.5.3",
        "@types/jest": "^24.0.11",
        "@types/react": "^16.8.7",
        "@types/react-dom": "^16.8.2",
        "@types/react-redux": "^7.0.5",
        "@types/react-test-renderer": "^16.8.1",
        "@types/redux": "^3.6.0",
        "@types/redux-mock-store": "^1.0.0",
        "@types/styled-components": "^4.1.12",
        "@types/webpack-env": "^1.13.9",
        "babel-loader": "^8.0.5",
        "babel-plugin-styled-components": "^1.10.0",
        "circular-dependency-plugin": "^5.0.2",
        "clean-webpack-plugin": "^2.0.0",
        "cross-env": "^5.2.0",
        "enzyme": "^3.9.0",
        "enzyme-adapter-react-16": "^1.11.2",
        "enzyme-to-json": "^3.3.5",
        "fork-ts-checker-webpack-plugin": "^1.0.0",
        "html-webpack-plugin": "^3.2.0",
        "jest": "^24.5.0",
        "jest-styled-components": "^6.3.1",
        "npm-run-all": "^4.1.5",
        "prettier": "^1.16.4",
        "react-hot-loader": "^4.8.0",
        "react-test-renderer": "^16.8.4",
        "react-testing-library": "^6.1.2",
        "redux-devtools-extension": "^2.13.8",
        "redux-mock-store": "^1.5.3",
        "stylelint": "^9.10.1",
        "stylelint-config-recommended": "^2.1.0",
        "stylelint-config-styled-components": "^0.1.1",
        "stylelint-custom-processor-loader": "^0.6.0",
        "stylelint-processor-styled-components": "^1.5.2",
        "thread-loader": "^2.1.2",
        "tsconfig-paths-webpack-plugin": "^3.2.0",
        "tslint": "^5.13.1",
        "tslint-config-airbnb": "^5.11.1",
        "tslint-config-prettier": "^1.18.0",
        "tslint-plugin-prettier": "^2.0.1",
        "tslint-react": "^3.6.0",
        "tslint-react-hooks": "^2.0.0-alpha.1",
        "typescript": "^3.3.3333",
        "webpack": "^4.29.6",
        "webpack-bundle-analyzer": "^3.1.0",
        "webpack-cli": "^3.2.3",
        "webpack-dev-server": "^3.2.1",
        "webpack-merge": "^4.2.1"
    }
}
